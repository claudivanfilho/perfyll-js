{
  "name": "perfyll",
  "version": "0.0.3",
  "description": "Perfyll is a lightweight JavaScript library designed to empower developers in tracking performance and user actions from an end-to-end (E2E) perspective.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "author": "Claudivan Filho",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "performance",
    "track",
    "mark",
    "RUM",
    "APM",
    "Real User Monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudivanfilho/perfyll-js.git"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A index",
    "postversion": "git push && git push --tags"
  },
  "prepublish": "tsc",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/ws": "^8.5.5",
    "amqplib": "^0.10.3",
    "body-parser": "^1.20.2",
    "bun-types": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "js-awe": "^1.0.58",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  },
  "dependencies": {
    "kleur": "^4.1.5",
    "ws": "^8.14.2"
  }
}
